datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // relationMode = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
  output          = "../../node_modules/.prisma/client"
}

model Test {
  id    String @id @default(uuid())
  label String

  @@map("test")
}
